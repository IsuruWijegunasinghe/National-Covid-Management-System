<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>PatientServlet</servlet-name>
        <servlet-class>sparkx.ncms.controller.PatientServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PatientServlet</servlet-name>
        <url-pattern>/Patient/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DoctorServlet</servlet-name>
        <servlet-class>sparkx.ncms.controller.DoctorServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DoctorServlet</servlet-name>
        <url-pattern>/Doctor/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HospitalServlet</servlet-name>
        <servlet-class>sparkx.ncms.controller.HospitalServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HospitalServlet</servlet-name>
        <url-pattern>/Hospital/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MoHServlet</servlet-name>
        <servlet-class>sparkx.ncms.controller.MoHServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MoHServlet</servlet-name>
        <url-pattern>/MoH/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>sparkx.ncms.controller.UserServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/Authentication/*</url-pattern>
    </servlet-mapping>

